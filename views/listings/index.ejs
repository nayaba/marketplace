<%- include('../partials/head') %>

    <h1>Marketplace Listings</h1>

    <ul>
        <% foundListings.forEach((listing) => { %>
            <li>
                <a href="/listings/<%= listing._id %>"><h2><%= listing.title %></h2></a>
                <p><%= listing.description %></p>
                <p><strong><%= listing.price %> BHD</strong></p>
                <img src="<%= listing.image %>" class="listing-img" alt="Image of <%= listing.title %>" />
            </li>
        <% }) %>
    </ul>
</body>
</html>